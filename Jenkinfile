node {
    try {
        stage('Checkout') {
        checkout scmGit(branches: [[name: '**'], [name: '**/*'], [name: '**'], [name: 'refs/tags/*']], extensions: [], userRemoteConfigs: [[credentialsId: 'github_credential', url: 'https://github.com/ngodanghuy162/jenkin_lab']])
            // git branch: 'main',
            //     credentialsId: 'github_credential',
            //     url: 'https://github.com/ngodanghuy162/jenkin_lab.git'
        env.GIT_URL = sh(script: "git config --get remote.origin.url", returnStdout: true).trim()
        env.GIT_BRANCH = sh(script: "git rev-parse --abbrev-ref HEAD", returnStdout: true).trim()
        env.GIT_COMMIT = sh(script: "git rev-parse HEAD", returnStdout: true).trim()
        }
        jenkinsfile_script = load 'loadfile1.groovy'
    
        stage('Info') {
            echo "Tên job: ${env.JOB_NAME}"
            echo "ID build: ${env.BUILD_ID}"
            echo "Từ nhánh: ${env.BRANCH_NAME}"
            echo "Commit hiện tại: ${env.GIT_COMMIT}"
            echo "Workspace: ${env.WORKSPACE}"
        }
    
        jenkinsfile_script.printMessage("test ms")
        jenkinsfile_script.printHello()
    } catch (Exception e) {
        // Xử lý lỗi
        echo "Đã xảy ra lỗi: ${e.message}"
        // Bạn có thể thực hiện các hành động bổ sung như thông báo, log lỗi, v.v.
    }
}
